<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">

    <description>
      Ginny
    </description>
    <display-name>Ginny</display-name>
   

    <!-- Define example application events listeners -->
    <listener>
        <listener-class>com.ginny.servicemanager.web.server.GinnyServiceManagerApplicationContextListener</listener-class>
    </listener>


    <!-- Define servlets that are included in the example application -->

    <servlet>
      <servlet-name>GinnyServiceManagerMainServelet</servlet-name>
      <servlet-class>com.ginny.servicemanager.web.server.GinnyServiceManagerMainServelet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>GinnyServiceManagerMainServelet</servlet-name>		
        <url-pattern>/*</url-pattern>
    </servlet-mapping>   

</web-app>
